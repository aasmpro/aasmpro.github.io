{
  "name": "abolfazlamiri",
  "version": "2.2.1",
  "private": true,
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "5.2.0",
    "react-scripts": "^5.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "css": "tailwindcss -i src/assets/styles/main.css -o src/assets/styles/main.min.css -m -w -p",
    "build:css": "tailwindcss -i src/assets/styles/main.css -o src/assets/styles/main.min.css -m",
    "build": "react-scripts build",
    "deploy": "yarn run build:css && yarn run build && gh-pages -d build",
    "gh-pages": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "gh-pages": "^4.0.0",
    "tailwindcss": "^3.1.4"
  }
}
